   "source": [
    "import pandas as pd\n",
    "\n",
    "df=pd.read_csv(r'C:\\bootcamo\\purchase_data.csv')\n",
    "\n",
    "df\n",
    "\n",
    "bins = [0,7,14,21,28,35,42,49]\n",
    "\n",
    "\n",
    "group_names = [\"A\", \"B\", \"C\", \"D\", \"E\",\"F\",\"G\"]\n",
    "\n",
    "df[\"Age Range\"] = pd.cut(df[\"Age\"], bins, labels=group_names)\n",
    "df\n",
    "\n",
    "df = df.groupby(\"Age Range\")\n",
    "df.count()\n",
    "\n",
    "\n",
    "\n"
   ]
